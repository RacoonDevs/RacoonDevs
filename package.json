{
  "name": "racoondevs-monorepo",
  "version": "1.0.0",
  "description": "RacoonDevs Landing Page - Full Stack Application",
  "private": true,
  "scripts": {
    "install-all": "cd front && npm install && cd ../back && npm install",
    "dev": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "dev:front": "cd front && npm run dev",
    "dev:back": "cd back && npm run dev",
    "build": "cd front && npm run build",
    "start": "cd back && npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "racoondevs",
    "landing-page",
    "react",
    "express",
    "contact-form"
  ],
  "author": "RacoonDevs",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
